<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.leesia.dataio.dao.ext.ExtProvinceMapper">
    <resultMap id="BaseResultMap" type="org.leesia.entity.Province">
        <id column="ID" jdbcType="CHAR" property="id"/>
        <result column="PROVINCE_NAME" jdbcType="VARCHAR" property="provinceName"/>
        <result column="CAPITAL" jdbcType="CHAR" property="capital"/>
        <result column="CAPITAL_NAME" jdbcType="VARCHAR" property="capitalName"/>
        <result column="CREATE_DATETIME" jdbcType="TIMESTAMP" property="createDatetime"/>
        <result column="UPDATE_DATETIME" jdbcType="TIMESTAMP" property="updateDatetime"/>
    </resultMap>

    <sql id="Base_Column_List">
        ID, PROVINCE_NAME, CAPITAL, CAPITAL_NAME, CREATE_DATETIME, UPDATE_DATETIME
    </sql>

    <insert id="bacthInsert">
        insert into PROVINCE (
            ID, PROVINCE_NAME, CAPITAL, CAPITAL_NAME, CREATE_DATETIME, UPDATE_DATETIME
        )
        values
        <foreach collection="list" item="data" separator=",">
            (
                #{data.id,jdbcType=CHAR},
                #{data.provinceName,jdbcType=VARCHAR},
                #{data.capital,jdbcType=CHAR},
                #{data.capitalName,jdbcType=VARCHAR},
                #{data.createDatetime,jdbcType=TIMESTAMP},
                #{data.updateDatetime,jdbcType=TIMESTAMP}
            )
        </foreach>
    </insert>
</mapper>